{
    "builds": [{
        "src": "app/wsgi.py",
        "use": "git+https://github.com/jgtvares/py-vercel.git#main",
        "config": {
            "maxLambdaSize": "30mb",
            "runtime": "python3.6"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/wsgi.py"
        }
    ]
}
